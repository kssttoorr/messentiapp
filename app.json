{
  "expo": {
    "name": "Contact Manager",
    "slug": "contact-manager",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.messenti",
      "config": {
        "usesNonExemptEncryption": false
      },
      "deploymentTarget": "15.1",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "package": "com.messenti",
      "googleServicesFile": "./android/app/google-services.json"
    },
    "scheme": "messenti",
    "plugins": [
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 33,
            "targetSdkVersion": 33,
            "buildToolsVersion": "33.0.0"
          },
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ],
      [
        "react-native-fbsdk-next",
        {
          "appID": "1902925610187308",
          "clientToken": "74088d564f1473a017e6ab5196978d53",
          "displayName": "Messenti",
          "scheme": "fb${Messenti}",
          "advertiserIDCollectionEnabled": false,
          "autoLogAppEventsEnabled": false,
          "isAutoInitEnabled": true,
          "iosUserTrackingPermission": "This identifier will be used to deliver personalized ads to you."
        }
      ]
    ],
    "web": {
      "config": {
        "firebase": {
          "apiKey": "AIzaSyD5u5UbQJtzqTj1iU5He_3Nkq2f5AVDcxo",
          "authDomain": "messenti.firebaseapp.com",
          "databaseURL": "https://messenti-default-rtdb.firebaseio.com",
          "projectId": "messenti",
          "storageBucket": "messenti.firebasestorage.app",
          "messagingSenderId": "240075430882",
          "appId": "1:240075430882:web:f96f4edcc41a16cec977bb"
        }
      }
    }
  }
}
